<script>
	jQuery(document).ready(function () {
		if (jQuery('#customer-stories').length) {
			jQuery('#customer-stories').mixItUp({
				selectors : {
					target : '.customer-story',
					filter : '.story-filters .filter',
				},
				animation : {
					duration : 300,
					effects : 'fade translateZ(-100px)'
				}
			});

			jQuery('.story-filters #regionSelect').on('change', function () {
				var region = (this.value == 'all') ? '' : this.value;
				jQuery('.story-filters .filter').each(function(){
					var tag = jQuery(this).attr('data-tag');
						tag = (tag == 'all') ? '' : tag;
					jQuery(this).attr('data-filter', (tag + region == '') ? 'all' : (tag + region));
				});
				var filter = jQuery('.story-filters .filter.active').attr('data-filter');
				jQuery.MixItUp.prototype._instances['customer-stories'].filter(filter);
			});
		}
	});
</script>
